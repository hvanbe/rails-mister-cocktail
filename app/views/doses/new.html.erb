<div class="container pt-5">
  <h1>Add a new dose</h1>
  <br>
    <%= simple_form_for [@cocktail, @dose] do |f| %>
      <%= f.association :ingredient %>
      <%= f.input :description %>
      <%= f.submit 'Add ingredient', class: 'btn btn-primary button_submit'%>
    <% end %>

  <%= link_to "Back to cocktail", cocktail_path(params[:cocktail_id]), class: 'btn btn-outline-dark button_back' %>
</div>




